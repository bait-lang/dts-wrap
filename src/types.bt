package main

struct Table {
mut:
	indexes map[string]i32
}

fun (mut t Table) register(name string) i32 {
	idx := t.indexes[name]
	if idx > 0 {
		return idx
	}

	new_idx := t.indexes.length
	t.indexes[name] = new_idx
	return new_idx
}
