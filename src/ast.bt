package main

type Decl := Interface | EmptyDecl

struct Module {
	name string
	decls []Decl
}

struct Interface {
	name string
	fields []Field
	methods []Method
}

struct Field {
	name string
	typ i32
}

struct Method {
	name string
	params []Field
	return_type i32
}

struct EmptyDecl{}
